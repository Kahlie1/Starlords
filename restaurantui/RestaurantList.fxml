         </children>
      </VBox>
      <VBox layoutX="250.0" layoutY="262.0" prefHeight="62.0" prefWidth="247.0">
         <children>
            <Label prefHeight="23.0" prefWidth="200.0" text="Address" />
            <Label fx:id="restaurantAddressLabel" alignment="TOP_LEFT" prefHeight="44.0" prefWidth="247.0" text="Label" wrapText="true" />
         </children>
      </VBox>
      <VBox layoutX="250.0" layoutY="335.0" prefHeight="74.0" prefWidth="247.0">
         <children>
            <Label prefHeight="26.0" prefWidth="200.0" text="URL" />
            <Hyperlink fx:id="restaurantURL" alignment="TOP_LEFT" prefHeight="53.0" prefWidth="247.0" text="Hyperlink" wrapText="true" />
         </children>
      </VBox>
      <Button layoutX="198.0" layoutY="450.0" mnemonicParsing="false" onAction="#backToCuisineScene" text="Back To Cuisines" />
   </children>
</AnchorPane>
